\ProvidesPackage{beamercolorthemesnow}
\RequirePackage{tumcolors}

\colorlet{highlight}{yellow}

\setbeamercolor*{palette primary}{fg=black,bg=white}
\setbeamercolor*{palette secondary}{parent=palette primary,fg=tumblau}
\setbeamercolor*{palette tertiary}{fg=tumadditionaldarkblue,bg=white} % fg sets font for navigation
\setbeamercolor*{palette quarternary}{fg=tumblau,bg=tumblau!15}

\setbeamercolor{alerted text}{fg=orange}
\setbeamercolor{normal text}{parent=palette primary}

\setbeamercolor{frametitle}{parent=palette primary}
\setbeamercolor{headline}{parent=palette secondary}
\setbeamercolor{titlelike}{parent=palette primary,fg=tumadditionaldarkblue}
\setbeamercolor{title}{fg=tumadditionalverydarkblue,bg=tumadditionalverylightblue}
\setbeamercolor{title page}{fg=tumadditionalverydarkblue}

\setbeamercolor{block title}{fg=white,bg=tumadditionaldarkblue}
\setbeamercolor{block body}{bg=tumadditionaldarkblue!15,fg=black}

\setbeamercolor{block title alerted}{fg=white,bg=tumadditionaldarkblue}
\setbeamercolor{block body alerted}{bg=tumadditionaldarkblue!15,fg=orange}

\setbeamercolor{block title example}{fg=white,bg=tumadditionaldarkblue}
\setbeamercolor{block body example}{bg=tumadditionaldarkblue!15,fg=tumadditionaldarkblue}

\setbeamercolor{structure}{fg=tumadditionaldarkblue,bg=white}

\setbeamercolor{item}{bg=white,fg=tumadditionaldarkblue}
\setbeamercolor{subitem}{bg=white,fg=tumadditionalblue}
\setbeamercolor{subsubitem}{bg=white,fg=tumadditionallightblue}

\setbeamercolor{item projected}{fg=white,bg=tumadditionaldarkblue}
\setbeamercolor{subitem projected}{fg=white,bg=tumadditionalblue}
\setbeamercolor{subsubitem projected}{fg=white,bg=tumadditionallightblue}

\setbeamercolor{description item}{fg=yellow}
\setbeamercolor{description subitem}{fg=yellow}
\setbeamercolor{description subsubitem}{fg=yellow}


