\ProvidesPackage{beamercolorthemesquid}
\RequirePackage{tumcolors}

\colorlet{highlight}{lightgreen}

\renewcommand{\tumias@slideStyle}{-a}

\setbeamercolor*{palette primary}{fg=white,bg=tumadditionaldarkblue}
\setbeamercolor*{palette secondary}{parent=palette primary,fg=tumadditionallightblue}
\setbeamercolor*{palette tertiary}{bg=tumadditionaldarkblue!90,fg=tumadditionallightblue} % fg sets font for navigation
\setbeamercolor*{palette quarternary}{bg=tumadditionaldarkblue!90,fg=tumadditionallightblue}

\setbeamercolor{alerted text}{fg=orange}
\setbeamercolor{normal text}{fg=white,bg=tumadditionaldarkblue}

\setbeamercolor{frametitle}{parent=palette primary}
\setbeamercolor{headline}{parent=palette primary}
\setbeamercolor{titlelike}{parent=palette primary,fg=white}
\setbeamercolor{title}{bg=tumadditionalverylightblue,fg=tumadditionaldarkblue}
\setbeamercolor{title page}{fg=white,bg=tumadditionaldarkblue}

\setbeamercolor{block title}{fg=tumadditionaldarkblue,bg=tumadditionaldarkblue!15}
\setbeamercolor{block body}{bg=tumadditionaldarkblue!5,fg=black}

\setbeamercolor{block title alerted}{fg=orange,bg=tumadditionaldarkblue!15}
\setbeamercolor{block body alerted}{bg=tumadditionaldarkblue!5,fg=black}

\setbeamercolor{block title example}{fg=green,bg=tumadditionaldarkblue!15}
\setbeamercolor{block body example}{bg=tumadditionaldarkblue!5,fg=black}

\setbeamercolor{structure}{bg=tumadditionaldarkblue,fg=white}

\setbeamercolor{item}{bg=white,fg=tumadditionalverylightblue}
\setbeamercolor{subitem}{bg=white,fg=tumadditionallightblue}
\setbeamercolor{subsubitem}{bg=white,fg=tumadditionalblue}

\setbeamercolor{item projected}{bg=tumadditionalblue,fg=white}
\setbeamercolor{subitem projected}{bg=tumadditionallightblue,fg=white}
\setbeamercolor{subsubitem projected}{bg=tumadditionalverylightblue,fg=tumadditionaldarkblue}

\setbeamercolor{description item}{fg=lightgreen}
\setbeamercolor{description subitem}{fg=lightgreen}
\setbeamercolor{description subsubitem}{fg=lightgreen}


